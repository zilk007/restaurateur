<h1>New photo</h1>

<%= simple_form_for @photo, :url => photos_path, html: {class: "form-signin"} do |f| %>

   <div class="row">
      <div class="col-xs-12 col-sm-offset-0 form-group"> 

<%= f.file_field :photo, placeholder: "Upload Photo"%>
</div></div>

    <div class="row">
      <div class="col-xs-5 col-sm-offset-2 form-group">

<%= f.button :submit, "Sign up", class: "btn btn-primary btn-block" %>
</div></div>

  <% end %>

